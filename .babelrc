{
  "presets": [
    "next/babel"
  ],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "plugins": ["inline-dotenv", "transform-inline-environment-variables"]
    },
    "test": {
      "presets": [[{ "preset-env": { "modules": "commonjs" } }]],
      "plugins": ["inline-dotenv"]
    }
  }
}
